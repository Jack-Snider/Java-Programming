package kr.or.ddit.basic;

import java.util.ArrayList;

public class ListSortTest {
	
	public static void main(String[] args) {
		
		ArrayList<String> list = new ArrayList<String>();
		
		list.add("일지매");
		list.add("홍길동");
		list.add("성춘향");
		list.add("변학도");
		list.add("이순신");
		
		
		System.out.println("정렬 전 : " + list);
		
		
	}
	
}
